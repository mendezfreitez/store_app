EAAA,UAAA,EAAA;;AAEA,UAAA,WAAA,GAAA,EAAA,IAAA,WAAA,GAAA,GAAA,EAAA;AACA,YAAA,WAAA,GAAA,EAAA,IAAA,WAAA,GAAA,EAAA,EAAA;AACA,UAAA,EAAA,GAAA,EAAA,CAAA,MAAA,CAAA,CAAA,EAAA,EAAA,CAAA,MAAA,GAAA,CAAA,CAAA;AACA;AACA;;AACA,WAAA,IAAA,CAAA,OAAA,GAAA,EAAA,CAAA,KAAA,CAAA,EAAA,EAAA,IAAA,CAAA,EAAA,CAAA;AACA,KAXA;AAYA,wBAAA,yBAAA,EAAA,EAAA,EAAA,EAAA;AACA,MAAA,EAAA,GAAA,EAAA,CAAA,WAAA,EAAA;AACA,UAAA,WAAA,GAAA,EAAA,CAAA,MAAA,CAAA,EAAA,CAAA,MAAA,GAAA,CAAA,EAAA,CAAA,EAAA,UAAA,EAAA;;AACA,UAAA,WAAA,GAAA,EAAA,IAAA,WAAA,GAAA,GAAA,EAAA;AACA,YAAA,WAAA,GAAA,EAAA,IAAA,WAAA,GAAA,EAAA,EAAA;AACA,cAAA,WAAA,GAAA,EAAA,IAAA,WAAA,GAAA,EAAA,EAAA;AACA,gBAAA,WAAA,IAAA,EAAA,IAAA,WAAA,IAAA,GAAA,IAAA,WAAA,IAAA,GAAA,EAAA;AACA,cAAA,EAAA,GAAA,EAAA,CAAA,MAAA,CAAA,CAAA,EAAA,EAAA,CAAA,MAAA,GAAA,CAAA,CAAA;AACA;AACA;AACA;AACA;;AACA,WAAA,IAAA,CAAA,WAAA,GAAA,EAAA,CAAA,KAAA,CAAA,EAAA,EAAA,IAAA,CAAA,EAAA,CAAA;AACA,KAzBA;AA0BA,8BAAA,+BAAA,EAAA,EAAA,EAAA,EAAA;AACA,MAAA,EAAA,GAAA,EAAA,CAAA,WAAA,EAAA;AACA,UAAA,WAAA,GAAA,EAAA,CAAA,MAAA,CAAA,EAAA,CAAA,MAAA,GAAA,CAAA,EAAA,CAAA,EAAA,UAAA,EAAA;;AACA,UAAA,WAAA,GAAA,EAAA,IAAA,WAAA,GAAA,GAAA,EAAA;AACA,YAAA,WAAA,GAAA,EAAA,IAAA,WAAA,GAAA,EAAA,EAAA;AACA,cAAA,WAAA,GAAA,EAAA,IAAA,WAAA,GAAA,EAAA,EAAA;AACA,gBAAA,WAAA,IAAA,EAAA,IAAA,WAAA,IAAA,GAAA,IAAA,WAAA,IAAA,GAAA,EAAA;AACA,cAAA,EAAA,GAAA,EAAA,CAAA,MAAA,CAAA,CAAA,EAAA,EAAA,CAAA,MAAA,GAAA,CAAA,CAAA;AACA;AACA;AACA;AACA;;AACA,WAAA,IAAA,CAAA,iBAAA,GAAA,EAAA,CAAA,KAAA,CAAA,EAAA,EAAA,IAAA,CAAA,EAAA,CAAA;AACA;AAvCA;AAtGA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <ModalTerminosCondiciones ref=\"ModalTerminosCondiciones\"></ModalTerminosCondiciones>\r\n    <ModalMensaje :titulo=\"titulo\" :texto=\"texto\" ref=\"ModalMensaje\"></ModalMensaje>\r\n    <b-modal id=\"modal_registro\"\r\n    style=\"border: 0px solid rgba(0, 0, 0, 0.2)!important;\"\r\n    title=\"Registro Usuario\"\r\n    header-close-variant=\"light\"\r\n    header-text-variant=\"light\"\r\n    cancel-variant=\"transparent\"\r\n    hide-footer\r\n    cancel-title=\"Cerrar\"\r\n    centered>\r\n      <div>\r\n        <div class=\"p-3\">\r\n          <b-form @submit=\"onSubmit\" @reset=\"onReset\" v-if=\"show\">\r\n            <b-form-group id=\"input-group-1\">\r\n              <input\r\n                class=\"form-control\"\r\n                id=\"input-1\"\r\n                v-model=\"form.usuario\"\r\n                type=\"text\" \r\n                required\r\n                placeholder=\"Usuario\"\r\n              >\r\n            </b-form-group>\r\n            <b-form-group id=\"input-group-4\">\r\n              <input\r\n                class=\"form-control\"\r\n                id=\"input-correo\"\r\n                v-model=\"form.correo\"\r\n                type=\"email\" \r\n                required\r\n                placeholder=\"Correo\"\r\n              >\r\n            </b-form-group>\r\n            <b-form-group id=\"input-group-2\">\r\n              <input\r\n                class=\"form-control\"\r\n                id=\"input-2\"\r\n                v-model=\"form.contrasenia\"\r\n                required\r\n                placeholder=\"Contraseña\"\r\n                type=\"password\"\r\n              >\r\n            </b-form-group>\r\n            <b-form-group id=\"input-group-3\">\r\n              <input\r\n                class=\"form-control\"\r\n                id=\"input-3\"\r\n                v-model=\"form.contraseniaRepeat\"\r\n                :state=\"contrasenias\"\r\n                required\r\n                placeholder=\"Repetir Contraseña\"\r\n                type=\"password\"\r\n              > \r\n              <div class=\"text-left\">\r\n                <b-form-invalid-feedback :state=\"contrasenias\">Contraseñas no coinciden</b-form-invalid-feedback>\r\n                <b-form-valid-feedback :state=\"contrasenias\">Contraseña Correcta</b-form-valid-feedback>\r\n              </div>\r\n            </b-form-group>\r\n            <b-form-group id=\"input-group-5\">\r\n              <b-form-checkbox-group class=\"text-left\" v-model=\"form.checked\" id=\"checkboxes-4\">\r\n                <b-form-checkbox @change=\"btnTerminos=!btnTerminos\" v-model=\"form.TerminosCondiciones\" required >Acepto los <b-link @click=\"modalTerminosCondiciones\">Términos y condiciones</b-link></b-form-checkbox>\r\n              </b-form-checkbox-group>\r\n            </b-form-group>\r\n            <div class=\"text-right\">\r\n              <b-button style=\"padding-left:12px!important; padding-right:12px!important;\" size=\"sm\" type=\"reset\" class=\"mr-2\" variant=\"outline-danger\">Limpiar</b-button>\r\n              <b-button style=\"padding-left:12px!important; padding-right:12px!important;\" :disabled=\"!btnTerminos\" size=\"sm\" type=\"submit\" class=\"ml-2\" variant=\"outline-dark\">Registrar</b-button>\r\n            </div>\r\n          </b-form>\r\n        </div>\r\n      </div>\r\n    </b-modal>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport ModalTerminosCondiciones from './ModalTerminosCondiciones'\r\nimport ModalMensaje from './Categorias/modalMensaje'\r\nlet url = 'https://storeapp-back-end.herokuapp.com/';\r\n  // let url = 'http://localhost:3000/';\r\n  import axios from 'axios';\r\n  export default {\r\n    name:'Registro', \r\n    props:{\r\n      desactivado: true\r\n    },\r\n    components:{\r\n      ModalTerminosCondiciones, ModalMensaje\r\n    },\r\n    data() {\r\n      return {\r\n        titulo:'',\r\n        texto:'',\r\n        btnTerminos:false,\r\n        form: {\r\n          usuario: '',\r\n          contrasenia: '',\r\n          correo:'',\r\n          contraseniaRepeat:'',\r\n          TerminosCondiciones:[false],\r\n          carro:[]\r\n        },\r\n        show: true,\r\n        estado:Boolean\r\n      }\r\n    },\r\n    methods: {\r\n      onSubmit(evt) {\r\n        evt.preventDefault();\r\n        if(this.form.TerminosCondiciones[0] == false){\r\n          alert(\"Antes debe aceptar los 'Términos y Condiciones'.\");\r\n        }\r\n        else{\r\n          var datos = this.form;\r\n          const config = {\r\n            headers: {'content-type': 'application/json'}\r\n          }\r\n          axios.post(`${url}Registro`, datos, config).then(function (resp) {\r\n              this.titulo = resp.data.titulo\r\n              this.texto = resp.data.texto\r\n              this.$bvModal.hide('modal_registro')\r\n              this.$bvModal.show('modal-mensaje')\r\n              // if(resp.data.titulo === \"Listo!\"){\r\n              //   that.onReset(evt);\r\n              //   alert(\"Usuario creado con éxito.\");\r\n              // }\r\n              // else{\r\n              //   alert(\"Error al crear usuario.\");\r\n              // }\r\n          }.bind(this));\r\n        }\r\n      },\r\n      onReset(evt) {\r\n        evt.preventDefault();\r\n        // Reset our form values\r\n        this.form.usuario = ''\r\n        this.form.contrasenia = ''\r\n        this.form.contraseniaRepeat = ''\r\n        this.$nextTick(() => {\r\n          this.show = true\r\n        })\r\n      },\r\n      validarUsuario(){\r\n        const config = {\r\n          headers: {'content-type': 'application/json'}\r\n        }\r\n        var vaina;\r\n        let that = this;\r\n        axios.post(`${url}disponibleUsuario`, that.form, config).then(function (resp) {\r\n            if(resp.data != '1'){\r\n              vaina = false;\r\n              that.estado = false;\r\n            }\r\n            else{\r\n              vaina = true;\r\n              that.estado = true;\r\n            }\r\n          console.log(vaina);\r\n        });\r\n        return vaina; \r\n      },\r\n      modalTerminosCondiciones(){\r\n        this.$bvModal.show('ModalTerminosCondiciones')\r\n      }\r\n    },\r\n    computed:{\r\n      contrasenias(){\r\n        if (this.form.contrasenia === this.form.contraseniaRepeat) {\r\n          this.estado = false;\r\n        } else {\r\n          this.estado = true;\r\n        }\r\n        return this.form.contrasenia === this.form.contraseniaRepeat;\r\n      },\r\n    },\r\n    mounted(){\r\n        if(this.form.contrasenia != ''){\r\n          this.estado = true;\r\n        }\r\n        else{\r\n          this.estado = false;\r\n        }\r\n    },\r\n    watch: {\r\n      'form.usuario': function(nv, ov){\r\n        nv = nv.toLowerCase()\r\n        var codigoAscii = nv.substr(nv.length - 1, 1).charCodeAt()\r\n        \r\n        if(codigoAscii < 97 || codigoAscii > 122){\r\n          if(codigoAscii < 48 || codigoAscii > 57){\r\n            nv = nv.substr(0, nv.length - 1)\r\n          }\r\n        }\r\n        this.form.usuario = nv.split('').join('');\r\n      },\r\n      'form.contrasenia': function(nv, ov){\r\n        nv = nv.toLowerCase()\r\n        var codigoAscii = nv.substr(nv.length - 1, 1).charCodeAt()\r\n        if(codigoAscii < 97 || codigoAscii > 122){\r\n          if(codigoAscii < 48 || codigoAscii > 57){\r\n            if(codigoAscii < 65 || codigoAscii > 90){\r\n              if(codigoAscii != 95 && codigoAscii != 196 && codigoAscii != 250){\r\n                nv = nv.substr(0, nv.length - 1)\r\n              }\r\n            }\r\n          }\r\n        }\r\n        this.form.contrasenia = nv.split('').join('');\r\n      },\r\n      'form.contraseniaRepeat': function(nv, ov){\r\n        nv = nv.toLowerCase()\r\n        var codigoAscii = nv.substr(nv.length - 1, 1).charCodeAt()\r\n        if(codigoAscii < 97 || codigoAscii > 122){\r\n          if(codigoAscii < 48 || codigoAscii > 57){\r\n            if(codigoAscii < 65 || codigoAscii > 90){\r\n              if(codigoAscii != 95 && codigoAscii != 196 && codigoAscii != 250){\r\n                nv = nv.substr(0, nv.length - 1)\r\n              }\r\n            }\r\n          }\r\n        }\r\n        this.form.contraseniaRepeat = nv.split('').join('');\r\n      }\r\n    }\r\n  }\r\n</script>\r\n\r\n<style>\r\n  .modal-body{\r\n    padding: 0px!important;\r\n  }\r\n  .close{\r\n    opacity:1!important;\r\n    font-weight: 900!important;\r\n    font-size: 40px!important;\r\n    padding-top:9px!important; \r\n    background-color: transparent!important;\r\n  }\r\n  .modal-header{\r\n    border-bottom: 0px!important;\r\n    background-color:#272727!important;\r\n  }\r\n  #modal_registro___BV_modal_content_{\r\n    border-width: 0px!important;\r\n  }\r\n  #modal_registro___BV_modal_title_{\r\n    padding-top: 5px!important;\r\n  }\r\n  .custom-checkbox, .custom-checkbox > label.custom-control-label{\r\n    width: 100%!important;\r\n  }\r\n</style>"],"sourceRoot":"src/components"}]}