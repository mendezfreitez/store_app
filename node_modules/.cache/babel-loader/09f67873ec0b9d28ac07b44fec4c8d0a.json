AlBA,mBAkBA,GAlBA,EAkBA;AAAA;;AACA,MAAA,GAAA,CAAA,cAAA,GADA,CAEA;;AACA,WAAA,IAAA,CAAA,OAAA,GAAA,EAAA;AACA,WAAA,IAAA,CAAA,WAAA,GAAA,EAAA;AACA,WAAA,SAAA,CAAA,YAAA;AACA,QAAA,KAAA,CAAA,IAAA,GAAA,IAAA;AACA,OAFA;AAGA;AA1BA;AAZA,CAAA","sourcesContent":["<template>\r\n  <b-container>\r\n      <b-row>\r\n          <b-col cols=\"1\" lg=\"4\" md=\"3\" sm=\"2\"></b-col>\r\n          <b-col cols=\"10\" lg=\"4\" md=\"6\" sm=\"8\">\r\n            <b-form @submit=\"onSubmit\" @reset=\"onReset\" v-if=\"show\">\r\n\r\n            <h3>Usuario</h3>\r\n\r\n            <b-form-group id=\"input-group-1\">\r\n              <b-form-input\r\n                id=\"input-1\"\r\n                v-model=\"form.usuario\"\r\n                type=\"text\" \r\n                required\r\n                placeholder=\"Usuario\"\r\n              ></b-form-input>\r\n            </b-form-group>\r\n\r\n            <b-form-group id=\"input-group-2\">\r\n              <b-form-input\r\n                id=\"input-2\"\r\n                v-model=\"form.contrasenia\"\r\n                required\r\n                placeholder=\"Contraseña\"\r\n                type=\"password\"\r\n              ></b-form-input>\r\n            </b-form-group>\r\n\r\n            <!-- <b-form-group id=\"input-group-3\" label=\"Food:\" label-for=\"input-3\">\r\n              <b-form-select\r\n                id=\"input-3\"\r\n                v-model=\"form.food\"\r\n                :options=\"foods\"\r\n                required\r\n              ></b-form-select>\r\n            </b-form-group> -->\r\n\r\n            <!-- <b-form-group id=\"input-group-4\">\r\n              <b-form-checkbox-group v-model=\"form.checked\" id=\"checkboxes-4\">\r\n                <b-form-checkbox value=\"me\">Check me out</b-form-checkbox>\r\n                <b-form-checkbox value=\"that\">Check that out</b-form-checkbox>\r\n              </b-form-checkbox-group>\r\n            </b-form-group> -->\r\n\r\n            <div class=\"text-right mb-3\">\r\n              <b-button type=\"reset\" class=\"mr-2\" variant=\"outline-danger\">Limpiar</b-button>\r\n              <b-button type=\"submit\" class=\"ml-2\" variant=\"outline-primary\">Ingresar</b-button>\r\n            </div>\r\n            <div class=\"text-center\">\r\n              <span>Aún no estás registrado? <b-link to=\"/Registro\">Regístrate</b-link></span>\r\n            </div>\r\n          </b-form>\r\n          <!-- <b-card class=\"mt-3\" header=\"Form Data Result\">\r\n            <pre class=\"m-0\">{{ form }}</pre>\r\n          </b-card> -->\r\n          </b-col>\r\n          <b-col cols=\"1\" lg=\"4\" md=\"3\" sm=\"2\"></b-col>\r\n      </b-row>\r\n  </b-container>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport { mapMutations } from 'vuex'\r\nlet url = 'http://localhost:3000/';\r\n  export default {\r\n    name:'Admin',\r\n    data() {\r\n      return {\r\n        form: {\r\n          usuario: '',\r\n          contrasenia: '',\r\n          carro: []\r\n        },\r\n        show: true\r\n      }\r\n    },\r\n    methods: {\r\n      ...mapMutations(['sesion_']),\r\n      onSubmit(evt) {\r\n        evt.preventDefault();\r\n        var datos = this.form;\r\n        const config = {\r\n          headers: {'content-type': 'application/json'}\r\n        }\r\n        let that = this;\r\n        axios.post(`${url}Acceso`, datos, config).then(function(resp){\r\n          console.log(resp.data.usuario[0]);\r\n          if(resp.data.resul === true){\r\n            that.sesion_({'resul':resp.data.resul, 'usuar':resp.data.usuario[0]});\r\n          }\r\n        });\r\n\r\n        // alert(JSON.stringify(this.form))\r\n      },\r\n      onReset(evt) {\r\n        evt.preventDefault()\r\n        // Reset our form values\r\n        this.form.usuario = ''\r\n        this.form.contrasenia = ''\r\n        this.$nextTick(() => {\r\n          this.show = true\r\n        })\r\n      }\r\n    } \r\n  }\r\n</script>\r\n\r\n<style>\r\n.container{\r\n    margin-top: 100px!important;\r\n}\r\na.nav-link{\r\n    color: #fff!important;\r\n}\r\n</style>"],"sourceRoot":"src/components"}]}