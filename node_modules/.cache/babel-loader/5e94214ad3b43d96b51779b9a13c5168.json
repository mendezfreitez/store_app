line-block!important;float:right!important;\">\r\n        Nuevo\r\n        <b-icon icon=\"plus\"></b-icon>\r\n    </b-button>\r\n    <div class=\"table-responsive\">\r\n        <table class=\"table table-sm table-bordered table-hover\">\r\n            <thead>\r\n                <tr>\r\n                    <th scope=\"col\">Producto</th>\r\n                    <th scope=\"col\">Descripción</th>\r\n                    <th scope=\"col\">Precio</th>\r\n                    <th scope=\"col\">Acción</th>\r\n                </tr>\r\n            </thead>\r\n            <tbody>\r\n                <tr v-for=\"Producto in productosTodos\" :key=\"Producto.idProducto\">\r\n                    <td nowrap class=\"text-left\">{{Producto.nombre}}</td>\r\n                    <td nowrap class=\"text-left\">{{Producto.descripcion.substr(0,50)}}</td>\r\n                    <td nowrap class=\"text-right\">{{Producto.precio | currency}}</td>\r\n                    <td nowrap class=\"text-center\">\r\n                        <a href=\"#\" class=\"papelera\" @click=\"eliminarProducto(Producto._id, Producto.nombre)\">\r\n                            <b-icon icon=\"trash\" variant=\"primary\" style=\"color: #7952b3;\"></b-icon>\r\n                        </a>\r\n                        <a href=\"#\" class=\"papelera ml-3\" @click=\"editarProducto(Producto._id)\">\r\n                            <b-icon icon=\"pencil\" variant=\"primary\" style=\"color: #7952b3;\"></b-icon>\r\n                        </a>\r\n                    </td>\r\n                </tr>\r\n            </tbody>\r\n        </table>\r\n    </div>\r\n  </b-container>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\nlet url = 'http://localhost:3000/';\r\nexport default {\r\n    name:'productosTodos',\r\n    data(){\r\n        return{\r\n            productosTodos:undefined,\r\n            Total:Number\r\n        }\r\n    },\r\n    mounted(){\r\n        let esto = this;\r\n        axios.get(`${url}traerTodos`).then(function(res){\r\n            esto.productosTodos = res.data;\r\n            console.log(esto.productosTodos);\r\n        });\r\n    },\r\n    methods:{\r\n        eliminarProducto(id, titulo){\r\n            alert(`¿Esta seguro de eliminar el producto '${titulo}'?`);\r\n        }, \r\n        editarProducto(id){\r\n            this.$router.push({ name:`editarProducto`, query:{ 'iden':id } });\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n</style>"],"sourceRoot":"src/components"}]}