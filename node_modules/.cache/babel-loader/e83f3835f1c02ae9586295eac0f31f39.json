CAAA,KAAA,cAAA,EAHA,CAGA;;AACA,WAAA,KAAA,GAAA,CAAA;AACA,WAAA,iBAAA;AACA,KAzBA;AA0BA,IAAA,iBA1BA,+BA0BA;AACA,WAAA,cAAA,CAAA,GAAA,CAAA,UAAA,CAAA,EAAA;AACA,aAAA,KAAA,IAAA,CAAA,CAAA,MAAA,GAAA,CAAA,CAAA,UAAA,CADA,CAEA;AACA,OAHA,CAGA,IAHA,CAGA,IAHA,CAAA,EADA,CAMA;AACA;AACA,KAlCA;AAmCA,IAAA,OAnCA,qBAmCA;AACA,MAAA,KAAA,gCAAA,QAAA,CAAA,aAAA,CAAA,aAAA,EAAA,WAAA,EAAA;AACA;AArCA,IAZA;AAmDA,EAAA,OAnDA,qBAmDA;AACA,QAAA,gBAAA,GAAA,IAAA,CAAA,KAAA,CAAA,YAAA,CAAA,OAAA,CAAA,gBAAA,CAAA,CAAA;AACA,SAAA,cAAA,CAAA,gBAAA;AACA,SAAA,KAAA,GAAA,CAAA;AAEA,SAAA,iBAAA;AACA,GAzDA;AA0DA,EAAA,KAAA,EAAA;AACA,IAAA,KADA,iBACA,KADA,EACA;AACA,WAAA,oBAAA,CAAA,KAAA;AACA;AAHA;AA1DA,CAAA","sourcesContent":["<template>\r\n    <div>\r\n        <div v-for=\"Producto in ProductosCarro\" :key=\"Producto.idProducto\" class=\"mb-2 mt-1 ml-2 mr-2\">\r\n            <b-card :img-src=\"Producto.imagen\" img-alt=\"Card image\" img-right img-height=\"100\" border-variant=\"secondary\" class=\"margenCard\">\r\n                <h6>{{Producto.tituloProducto}}</h6>\r\n                <div>\r\n                    <vue-numeric-input\r\n                    v-model=\"Producto.laCantidad\"\r\n                    align=\"center\"\r\n                    @change=\"cantidadProductoCarro(Producto.idProducto)\"\r\n                    :min=\"1\"\r\n                    :max=\"1000\" \r\n                    :id=\"Producto.idProducto\"\r\n                    size=\"100px\">\r\n                    </vue-numeric-input>\r\n\r\n                    <!-- <b-form-spinbutton\r\n                        inline\r\n                        :ref=\"'dato_'\"\r\n                        @change=\"cantidadProductoCarro(Producto.idProducto)\"\r\n                        :id=\"Producto.idProducto\" v-model=\"Producto.laCantidad\"\r\n                        min=\"1\"\r\n                        max=\"1000\"  \r\n                        style=\"height:23px!important; width:130px!important; margin-right:4px!important; border-color:green;\">\r\n                    </b-form-spinbutton> -->\r\n                </div>\r\n\r\n                <div>\r\n                    <b-badge class=\"text-right\" variant=\"success\" style=\"display:inline-block!important; font-size:13px; width:130px!important;\">\r\n                        {{Producto.precio * Producto.laCantidad | currency}}\r\n                    </b-badge>\r\n                </div>\r\n\r\n                <b-link style=\"position:absolute; bottom:5px!important; right:105px!important;\" class=\"btn btn-sm btn-outline-secondary papelera\" title=\"Eliminar Producto de Carro\" href=\"#\"  @click=\"removerDeCarro(Producto.idProducto, Producto.tituloProducto)\">\r\n                    Eliminar\r\n                </b-link>\r\n            </b-card>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import {mapState, mapMutations} from 'vuex'\r\n    export default {\r\n        name:'ListaProductos',\r\n        data(){\r\n            return{\r\n                losProductos: Array,\r\n                Total: Number,\r\n                elTotal:''\r\n            }\r\n        },\r\n        computed:{\r\n        ...mapState(['ProductosCarro','textoTotalCarro']),\r\n        },\r\n        methods:{\r\n        ...mapMutations(['modifCantProducto','modificarCarro','modifTextoTotalCarro']),\r\n            removerDeCarro(idProducto,tituloProducto){\r\n                var eliminar = confirm(`EstÃ¡ seguro de eliminar el producto '${tituloProducto}' de su carro?`)\r\n                if(eliminar){\r\n                    // console.log(this.ProductosCarro);\r\n                    let index = 0;\r\n                    this.ProductosCarro.forEach(element => {\r\n                        if(idProducto === element.idProducto){\r\n                            // console.log(`${idProducto} === ${element.idProducto}`)\r\n                            this.ProductosCarro.splice(index, 1);\r\n                            this.modificarCarro(this.ProductosCarro)  \r\n                        }\r\n                        index++;\r\n                    });\r\n                    this.Total = 0;\r\n                    this.calculoTotalCarro();\r\n                }\r\n            },\r\n            cantidadProductoCarro(dato){\r\n                alert()\r\n                let cantidad = document.getElementById(dato).value;\r\n                this.modificarCarro(this.ProductosCarro);       /////////////////////////////////////////////////////////////////////\r\n                this.Total = 0;\r\n                this.calculoTotalCarro();\r\n            },\r\n            calculoTotalCarro(){\r\n                this.ProductosCarro.map(function(e){\r\n                    this.Total += e.precio * e.laCantidad;\r\n                    // console.log(`Precio: ${e.precio} // Cantidad: ${e.laCantidad} // Total: ${e.precio*e.laCantidad}`);\r\n                }.bind(this));\r\n                \r\n                // document.getElementById('footerModal').innerHTML = this.Total;\r\n                // alert(numeral(this.Total).format('0,0'));\r\n            },\r\n            comprar(){\r\n                alert(`El total a pagar es: ${document.querySelector('#textoTotal').textContent}`);\r\n            }\r\n        },\r\n        mounted(){\r\n            var productosLocales = JSON.parse(localStorage.getItem('productosCarro'));\r\n            this.modificarCarro(productosLocales);\r\n            this.Total = 0;\r\n            \r\n            this.calculoTotalCarro();\r\n        },\r\n        watch:{\r\n            Total(nuevo){\r\n                this.modifTextoTotalCarro(nuevo);\r\n            }\r\n        }\r\n    }\r\n</script>\r\n\r\n<style>\r\n    #btnCompra{\r\n        height: 23px!important;\r\n        padding-top: 0px!important;\r\n    }\r\n    path.text-primary{\r\n        color: #FFC107!important;\r\n    }\r\n    .total:hover{\r\n        background-color: transparent!important;\r\n    }\r\n    .noBorder{\r\n        border-style:none!important;\r\n    }\r\n    td, th{\r\n        font-size: 13px!important;\r\n    }\r\n    .card-text, .b-form-spinbutton{\r\n        margin-bottom: 0px!important;\r\n        font-size: 13px!important;\r\n    }\r\n    .iconoEliminar:hover{\r\n        color: #fff!important;\r\n    }\r\n    .papelera{\r\n        height: 24px!important;\r\n        width: 70px!important;\r\n        padding: 0px!important;\r\n    }\r\n    div.margenCard > .card-body{\r\n        padding-top: 2px!important;\r\n        padding-left: 6px!important;\r\n    }\r\n    div.b-form-spinbutton{\r\n        height: 19px !important;\r\n    }\r\n</style>"],"sourceRoot":"src/components"}]}